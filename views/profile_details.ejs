    <script>
        $(document).ready(function(){
            $(".right.box-container .tabpage").each(function(e) {
                if (e != 0) $(this).hide();
            });
        
            $("#next").click(function(){
                if ($(".right.box-container .tabpage:visible").next().length != 0)
                    $(".right.box-container .tabpage:visible").next().show().prev().hide();
                else {
                    $(".right.box-container .tabpage:visible").hide();
                    $(".right.box-container .tabpage:first").show();
                }
                return false;
            });
        
            $("#prev").click(function(){
                if ($(".right.box-container .tabpage:visible").prev().length != 0)
                    $(".right.box-container .tabpage:visible").prev().show().next().hide();
                else {
                    $(".right.box-container .tabpage:visible").hide();
                    $(".right.box-container .tabpage:last").show();
                }
                return false;
            });
        });
    </script>

